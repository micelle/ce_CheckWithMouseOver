{
    "manifest_version": 2,
    "name": "CheckWithMouseOver",
    "version": "1.0.0",
    "description": "Invert the check with Alt key & mouse over for check box or radio button.",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/index.js"],
            "run_at": "document_end",
            "all_frames": true
        }
    ]
}